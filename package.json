{
  "name": "rif",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "docker-priv": "docker-compose exec db mysql -uroot -p -e 'GRANT ALL PRIVILEGES ON *.* TO 'root'@'%';'",
    "docker-up": "docker-compose up -d",
    "docker-stop": "docker-compose stop",
    "typeorm:migration": "npx typeorm migration:create",
    "typeorm:seed": "npx typeorm seed:create",
    "start": "node dist/index.js",
    "migrations:reverse": "npx typeorm-model-generator -h localhost -p 5432 -d rif -u root -x 'root' -e postgres -o ./src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JamesTellezTalero/rif.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/JamesTellezTalero/rif/issues"
  },
  "homepage": "https://github.com/JamesTellezTalero/rif#readme",
  "dependencies": {
    "body-parser": "^1.20.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "path": "^0.12.7",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.11",
    "typeorm-model-generator": "^0.4.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.14",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.12",
    "nodemon": "^2.0.16",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  }
}
